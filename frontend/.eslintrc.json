{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint-config-trybe-frontend",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/no-named-as-default-member": "off",
        "import/no-named-as-default": "off",
        "import/named": "off"
    }
}
